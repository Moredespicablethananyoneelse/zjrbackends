##修改测试脚本中的python虚拟环境建立的报错问题
 17 set_up() {
 18     MY_VENV="$(mktemp -d)"
 19     if [ -z "$MY_VENV" ]
 20     then
 21         echo "Failed to set up virtualenv." >&2
 22         exit 2
 23     fi
 24     #virtualenv -q "$MY_VENV"
 25     python -m venv  "$MY_VENV"
 26 
 27     # shellcheck disable=SC1091
 28     . "$MY_VENV/bin/activate"
 29     pip install -q pyflakes ruff shellcheck-py
 30 }

